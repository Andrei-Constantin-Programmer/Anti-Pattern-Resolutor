{
    "ollama": {
        "llm_provider": "ollama",
        "llm_model": "granite3.3:8b",
        "embedding_model": "nomic-embed-text:latest"
    },
    "ibm": {
        "llm_provider": "ibm",
        "llm_model": "ibm/granite-3-3-8b-instruct",
        "embedding_model": "nomic-embed-text:latest",
        "parameters": {
            "decoding_method": "sample",
            "max_new_tokens": 4000,
            "min_new_tokens": 1,
            "temperature": 0.5,
            "top_k": 50,
            "top_p": 1
        }
    }
}